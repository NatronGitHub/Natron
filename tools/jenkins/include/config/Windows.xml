<?xml version="1.0" encoding="UTF-8"?>
<Installer>
	<Name>Natron</Name>
	<Version>_VERSION_</Version>
	<Title>Natron</Title>
	<Publisher>INRIA</Publisher>
	<StartMenuDir>Natron</StartMenuDir>
	<ProductUrl>http://www.natron.fr</ProductUrl>
	<RemoveTargetDir>true</RemoveTargetDir>
	<RunProgram></RunProgram>
	<AllowSpaceInPath>true</AllowSpaceInPath>
	<UninstallerName>NatronSetup</UninstallerName>
	<InstallerWindowIcon>icon.png</InstallerWindowIcon>
	<Logo>logo.png</Logo>
	<Watermark>watermark.png</Watermark>
	<TargetDir>@ApplicationsDir@/_APP_INSTALL_SUFFIX_</TargetDir>
	<RemoteRepositories>
                <Repository>
                        <Url>_REMOTE_URL_/_REMOTE_PATH_</Url>
                        <Enabled>1</Enabled>
                        <DisplayName>Natron _RBVERSION_ Patch Releases</DisplayName>
                </Repository>
	</RemoteRepositories>
</Installer>
